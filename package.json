{
  "name": "ai-chat",
  "version": "0.1.0",
  "private": true,
  "scripts": {
    "dev": "next dev",
    "build-skipdb": "next build",
    "build": "npm run prisma-push && npm run prisma-generate && next build",
    "start": "next start",
    "lint": "next lint",
    "prisma-push": "prisma db push --schema=./src/prisma/schema.prisma",
    "prisma-studio": "prisma studio --schema=./src/prisma/schema.prisma",
    "prisma-migrate": "prisma migrate dev --schema=./src/prisma/schema.prisma --name init",
    "prisma-generate": "prisma generate --schema=./src/prisma/schema.prisma"
  },
  "dependencies": {
    "@ai-sdk/openai": "^0.0.33",
    "@auth/prisma-adapter": "^2.1.0",
    "@hookform/resolvers": "^3.4.2",
    "@prisma/client": "^5.15.0",
    "@radix-ui/react-avatar": "^1.0.4",
    "@radix-ui/react-dialog": "^1.0.5",
    "@radix-ui/react-dropdown-menu": "^2.0.6",
    "@radix-ui/react-label": "^2.0.2",
    "@radix-ui/react-separator": "^1.0.3",
    "@radix-ui/react-slot": "^1.0.2",
    "@radix-ui/react-tooltip": "^1.0.7",
    "ai": "^3.1.14",
    "class-variance-authority": "^0.7.0",
    "clsx": "^2.1.1",
    "dayjs": "^1.11.11",
    "groq-sdk": "^0.4.0",
    "lucide-react": "^0.379.0",
    "markdown-to-jsx": "^7.4.7",
    "next": "14.2.3",
    "next-auth": "^5.0.0-beta.18",
    "openai": "^4.52.0",
    "react": "^18",
    "react-dom": "^18",
    "react-hook-form": "^7.51.5",
    "sonner": "^1.4.41",
    "tailwind-merge": "^2.3.0",
    "tailwindcss-animate": "^1.0.7",
    "zod": "^3.23.8",
    "zustand": "^4.5.2"
  },
  "devDependencies": {
    "@svgr/webpack": "^8.1.0",
    "@types/node": "^20",
    "@types/react": "^18",
    "@types/react-dom": "^18",
    "eslint": "^8",
    "eslint-config-next": "14.2.3",
    "postcss": "^8",
    "prisma": "^5.15.0",
    "tailwindcss": "^3.4.1",
    "typescript": "^5"
  }
